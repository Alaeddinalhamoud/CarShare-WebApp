@model CarShare.Data.Order
@{
    ViewData["Title"] = "OrderConfirmation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!--== Page Title Area Start ==-->
<section id="page-title-area" class="section-padding overlay">
    <div class="container">
        <div class="row">
            <!-- Page Title Start -->
            <div class="col-lg-12">
                <div class="section-title  text-center">
                    <h2>Your payment has been confirmed.</h2>
                    <br />
                    <span class="title-line"><i class="fa fa-cart-plus fa-5x"></i></span>
                    <p>you order Number# @Model.Id.ToString()</p>
                    <br/>
                    <a style="color:goldenrod;" href="/Vehicle/VehicleDetails/@Model.VehicleId">Please, write your feedback</a>
                </div>
            </div>
            <!-- Page Title End -->
        </div>
    </div>
</section>





 



